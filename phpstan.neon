includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:
    tmpDir: cache

    paths:
        - app

    scanFiles:

    # The level 9 is the highest level
    level: 7

    parallel:
        maximumNumberOfProcesses: 4

    ignoreErrors:
        - identifier: missingType.iterableValue

    checkFunctionNameCase: true
    checkUninitializedProperties: true
